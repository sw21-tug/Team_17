<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginsesame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ls_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ls_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginSesame">
        <activity android:name=".CreateNewUserActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".EditVaultEntry"></activity>
        <activity android:name=".CreateVaultEntry"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>